<h1>Booking: <%= @booking.id %></h1>
<h3>Passengers:</h3>
<% @booking.passengers.each do |passenger| %>
  <article><strong>Name:</strong> <%= passenger.name %> <strong>Email:</strong> <%= passenger.email %><article> 
<% end %>
<%= button_to "Pay Now!", checkout_create_path, params: {id: @booking.id}, remote: true %>
